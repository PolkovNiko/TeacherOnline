@model Praktika.Models.Estimate
<h2>@ViewData["Title"]</h2>

<form asp-controller="Subject" asp-action="CreateSub" method="post">
    <div class="mb-3">
        <label class="form-label">Студент</label>
        <select class="form-control" asp-for="@Model.IdUser" asp-items="Model.user"></select>
        <label class="form-label">Предмет</label>
        <select class="form-control" asp-for="@Model.IdSubject" asp-items="Model.subj"></select>
        <label class="form-label">Отметка</label>
        <input type="text" class="form-control" asp-for="@Model.Score">
        <label class="form-label">Преподаватель</label>
        <select class="form-control" asp-for="@Model.IdTeacher" asp-items="Model.teach"></select>
    </div>
    <button type="submit" class="btn btn-primary">Создать пользователя</button>
</form>